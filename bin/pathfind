#!env perl

# PODNAME: pathfind

use strict;
use warnings;

use Path::Class;
use Bio::Path::Find;

# get a finder
my $pf = Bio::Path::Find->new(
  config_file => 't/data/06_finder/test.conf',
  environment => 'test',
);

$pf->print_paths(
  id       => '5477_6',
  type     => 'lane',
  filetype => 'fastq',
);

$DB::single = 1;

